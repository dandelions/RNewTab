{
    "manifest_version": 3,
    "name": "Aesthetic New Tab",
    "version": "1.0.0",
    "description": "A modern, aesthetic new tab page with cloud sync.",
    "icons": {
        "128": "logo.png"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "storage",
        "unlimitedStorage"
    ],
    "host_permissions": [
        "https://api.unsplash.com/*",
        "https://*.workers.dev/*"
    ]
}